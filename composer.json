{
    "name": "slowestwind/laravel-tab-session-guard",
    "description": "A Laravel package that allows developers to restrict and control how many tabs, sessions, and resource windows a user can open",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "session",
        "tab",
        "guard",
        "restriction",
        "security"
    ],
    "authors": [
        {
            "name": "Arvind Sharma",
            "email": "slowestwind@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0|^11.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "SlowestWind\\TabSessionGuard\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SlowestWind\\TabSessionGuard\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SlowestWind\\TabSessionGuard\\Providers\\TabSessionGuardServiceProvider"
            ],
            "aliases": {
                "TabGuard": "SlowestWind\\TabSessionGuard\\Facades\\TabGuard"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
